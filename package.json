  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lerachukovich/travel-app/issues"
  },
  "homepage": "https://github.com/lerachukovich/travel-app#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootswatch": "^4.6.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.7.1",
    "material-components-web": "^10.0.0",
    "mongoose": "^5.11.15",
    "node-sass": "^5.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react-leaflet": "^3.1.0",
    "react-slick": "^0.28.1",
    "reactstrap": "^8.9.0",
    "slick-carousel": "^1.8.1",
    "validator": "^13.5.2"
  },
